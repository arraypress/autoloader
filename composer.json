{
  "name": "arraypress/autoloader",
  "description": "A version-aware autoloader manager for ArrayPress libraries",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "homepage": "https://github.com/arraypress/autoloader/",
  "authors": [
    {
      "name": "David Sherlock",
      "homepage": "https://arraypress.com/"
    }
  ],
  "require": {
    "php": ">=7.4"
  },
  "autoload": {
    "psr-4": {
      "ArrayPress\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/arraypress/autoloader/issues",
    "source": "https://github.com/arraypress/autoloader"
  },
  "keywords": [
    "autoloader",
    "version-aware",
    "wordpress",
    "library",
    "package",
    "dependency",
    "management"
  ]
}